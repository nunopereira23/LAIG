                radius="15" startAng="0" rotAng="-90"/>

    <ANIMATION id="ambushLinear2" speed="15" type="linear">
        <controlpoint xx="35" yy="0" zz="25" />
        <controlpoint xx="25" yy="0" zz="25" />
    </ANIMATION>

    <ANIMATION id="convoyAnim" type="combo">
        <SPANREF id="convoyBezier1" />
        <SPANREF id="convoyLinear" />
        <SPANREF id="convoyBlowUp" />
    </ANIMATION>

    <ANIMATION id="ambushAnim" type="combo">
        <SPANREF id="ambushLinear1" />
        <SPANREF id="ambushCirc" />
        <SPANREF id="ambushLinear2" />
    </ANIMATION>

</ANIMATIONS>

