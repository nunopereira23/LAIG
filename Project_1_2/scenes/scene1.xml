<!-- The animations -->
<ANIMATIONS>
    <ANIMATION id="transTest" speed="8" type="linear">
        <controlpoint xx="-4" yy="0" zz="0" />
        <controlpoint xx="0" yy="0" zz="0" />
    </ANIMATION>

    <ANIMATION id="circTest" speed="5" type="circular"
               centerX = "0" centerY = "0" centerZ = "0"
               radius = "5" startAng = "0" rotAng = "90"/>

    <ANIMATION id="transTest2" speed="1" type="linear">
        <controlpoint xx="4" yy="0" zz="0" />
        <controlpoint xx="4" yy="0" zz="4" />
    </ANIMATION>

    <ANIMATION id="bezierTest" speed="8" type="bezier">
        <controlpoint xx="0" yy="0" zz="0" />
        <controlpoint xx="8" yy="0" zz="0" />
        <controlpoint xx="8" yy="0" zz="4" />
        <controlpoint xx="0" yy="0" zz="4" />
    </ANIMATION>

	<ANIMATION id="comboTest" type="combo">
		<SPANREF id="transTest" />
		<SPANREF id="bezierTest" />
    <SPANREF id="transTest" />

	</ANIMATION>

    <ANIMATION id="convoyBezier1" speed="10" type="bezier">
        <controlpoint xx="-10" yy="0" zz="-10" />
        <controlpoint xx="-5" yy="0" zz="-10" />
        <controlpoint xx="0" yy="0" zz="-5" />
        <controlpoint xx="0" yy="0" zz="0" />
    </ANIMATION>

    <ANIMATION id="convoyLinear" speed="15" type="linear">
        <controlpoint xx="0" yy="0" zz="0" />
        <controlpoint xx="0" yy="0" zz="20" />
    </ANIMATION>

    <ANIMATION id="convoyBlowUp" speed="15" type="bezier">
        <controlpoint xx="0" yy="0" zz="20" />
        <controlpoint xx="0" yy="10" zz="20" />
        <controlpoint xx="0" yy="10" zz="30" />
        <controlpoint xx="0" yy="0" zz="30" />
    </ANIMATION>

    <ANIMATION id="ambushLinear1" speed="15" type="linear">
        <controlpoint xx="50" yy="0" zz="0" />
        <controlpoint xx="50" yy="0" zz="10" />
    </ANIMATION>

    <ANIMATION id="ambushCirc" speed="15" type="circular"
